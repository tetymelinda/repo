#include <iostream>
using namespace std;

int main() {
    // Membuat array kosong berkapasitas 5
    int nilai[5];

    // Mengisi array kosong
    nilai[0] = 32;
    nilai[1] = 42;
    nilai[2] = 76;
    nilai[3] = 31;
    nilai[4] = 57;

    // Mencetak isi array dengan perulangan
    for(int i = 0; i < 5; i++) {
        cout << "Nilai ke-" << i + 1 << " : " << nilai[i] << endl;
    }

    return 0;
}

    
