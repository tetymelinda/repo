#include <iostream>
using namespace std;

int main()
{
    bool b = true;             // inisialisasi variabel b dengan nilai kebenaran "true"
    int i = 5;                 // inisialisasi variabel i dengan nilai bilangan butat 18
    float f = 3.14;            // inisialisasi variabel f dengan nilai desimal 3.14
    double d = 3.141592;       // inisialisasi variabel d dengan nilai desimal 3.141592
    char c = 'A';              // inisialisasi variabel c dengan karakter A
    string s = "Selamat Pagi"; // inisialisasi variabel s dengan kata Selamat Pagi
    return 0;
}
